%html
  %head
    %meta{:charset => "utf-8"}/
    %meta{:"http-equiv" => "X-UA-Compatible" , :content=>"IE=edge"}/
    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1"}/
    %meta{:name => "description", :content => "Envíos a todo México de productos locales auténticos de cada Estado."}/
    %meta{:name => "author", :content => "Mi Alacena"}/
    %title= "Mi Alacena"

    =favicon_link_tag 'favicon.png'

    <!-- for Facebook -->         
    %meta{:property => "og:title", :content=>"MiAlacena.mx"}/
    %meta{:property => "og:type", :content=>"website"}/
    %meta{:property => "og:image", :content=>"#{asset_url 'logo-f-fb.jpg'}" }/
    %meta{:property => "og:url", :content=>"http://mialacena.mx/"}/
    %meta{:property => "og:description", :content=>"Envíos a todo México de productos locales auténticos de cada Estado."}/

    <!-- for Twitter -->          
    %meta{:property => "twitter:card", :content=>"summary"}/
    %meta{:property => "twitter:title", :content=>"MiAlacena.mx"}/
    %meta{:property => "twitter:description", :content=>"Envíos a todo México de productos locales auténticos de cada Estado."}/
    %meta{:property => "twitter:image", :content=>"#{asset_url 'logo-f-fb.jpg'}" }/

    = stylesheet_link_tag "http://fonts.googleapis.com/css?family=Oswald"
    = stylesheet_link_tag :application, :media => "all", "data-turbolinks-track" => true
    = javascript_include_tag :application, "data-turbolinks-track" => true
    = csrf_meta_tags
  %body
    = yield

    .container

      %hr/

      
      .row.footer
        .col-md-3.col-xs-12
          %address
            MI ALACENA DE MEXICO LOGISTICA SAPI DE CV 
            %br/
            IGNACIO ALLENDE 21 AMPLIACION TORRE BLANCA 
            %br/
            CP 11289, CIUDAD DE MEXICO

        .col-md-9.col-xs-12.text-right
          %p
            CON APOYO DE
          %ul.list-inline
            %li
              =link_to image_tag("sum.png", :class => "img-responsive img-center"), "http://www.startupmexico.com/startup_mialacena", :target => "_blank"
            %li
              =image_tag("inadem.png", :class => "img-responsive img-center")
            %li
              =image_tag("cdmx.png", :class => "img-responsive img-center")

